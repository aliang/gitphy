{
  "name": "gitphy",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": [
        "/css/bootstrap-popover.css",
        "/css/style.css"
      ],
      "js": [
        "/index.bundle.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": ["https://github.com/*"]
}
