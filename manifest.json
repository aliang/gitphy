{
  "name": "gitphy",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Put gifs in your Github issues and pull requests without leaving the textarea",
  "author": "Kevin Wu",
  "icons": {
    "32": "src/icons/icon32.png",
    "64": "src/icons/icon64.png",
    "128": "src/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["dist/index.css"],
      "js": ["dist/index.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["https://github.com/*"],
  "web_accessible_resources": ["src/images/giphy_attribution.png"]
}
